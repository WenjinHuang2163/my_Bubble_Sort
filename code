funtion myBubbleSort(x)

	if length(x) < 1
	    return "Your original is not an array!"
	else
	    println(" original: $x")	
	    s=x						# Initialize s (sorted array)
	    index = [1:length(s)...]			# Initialize index (the index of elements in the sorted array)
	    rank = 0 * index				# Initialize rank (the ranks of elements in the original array)
	    for i in 2:length(s), j in 1:length(s)-1		# iterate: starting bubble sort
	        if s[j] > s[j+1]
			s[j], s[j+1] = s[j+1], s[j]		# swap if s[j] > s[j+1]
			index[j], index[j+1] = index[j+1], index[j]	# swap indices if s[j] > s[j+1]
	        end
	    end
	    for i in 1:length(index)			# iterate: find the rank
	        rank[index[i]] = i			# use the relationship between "index" and "rank" to find ranks
	    end
	end
	println(" sorted:   $s\n index:    $index\n rank:     $rank")
	
end
